#!/usr/bin/env node

            require( 'sn-core' );
var dudley = require( '../dudley' );

require( 'sn-props' ).read( function( props ) {
  ( new dudley( props ) ).process( function( err ) {
    if( err ) {
      process.stderr.write( err.toString() + "\n" );
      return process.exit( 2 );
    }
  } );
} );